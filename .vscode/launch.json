{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run current file as module (WSL)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "python": "${workspaceFolder}/venv/bin/python"
    }
  ]
}
